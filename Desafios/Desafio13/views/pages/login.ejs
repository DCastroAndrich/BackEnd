<!DOCTYPE html>
<html lang="es">
  <head>
    <%- include("../partials/head.ejs") %>
  </head>

  <body>
    <div class="container my-5">
      <div class="jumbotron">
        <h1 style="color: blue">Login de Usuario</h1>
        <br />
        <form
          class="form"
          action="/login"
          method="post"
          role="form"
          autocomplete="off"
        >
          <div class="form-group">
            <input
              id="name"
              name="name"
              placeholder="Ingrese su nombre de usuario"
              class="form-control"
              type="text"
              required
            />
          </div>

          <div class="form-group">
            <input class="btn btn-success my-3" type="submit" value="Enviar" />
          </div>
        </form>
      </div>
    </div>
  </body>
</html>
